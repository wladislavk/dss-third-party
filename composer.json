{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6",
        "laravel/framework": "5.1.*",
        "eventhomes/laravel-fractalhelper": "^0.1.2",
        "tymon/jwt-auth": "^0.5.4",
        "zircote/swagger-php": "^2.0",
        "rmccue/requests": ">=1.0",
        "guzzlehttp/guzzle": "~6.0",
        "dompdf/dompdf": "^0.7.0",
        "barryvdh/laravel-dompdf": "^0.7.1",
        "vkr/signature-to-image": "^1.2",
        "barryvdh/laravel-ide-helper": "^2.3",
        "doctrine/dbal": "~2.3",
        "prettus/l5-repository": "^2.6"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7",
        "phpspec/phpspec": "~2.1",
        "laracasts/generators": "^1.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "DentalSleepSolutions\\": ["app/", "app/DentalSleepSolutions/"],
            "Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCases/ApiTestCase.php",
            "tests/TestCases/UnitTestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
